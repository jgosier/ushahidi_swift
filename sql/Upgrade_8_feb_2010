  `id` bigint(20) unsigned NOT NULL AUTO_INCREMENT,
  `incident_id` int(11) DEFAULT '0',
  `tweet_from` varchar(100) DEFAULT NULL,
  `tweet_to` varchar(100) DEFAULT NULL,
